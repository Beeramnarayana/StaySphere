{
  "name": "ai-rental-housing",
  "version": "1.0.0",
  "description": "AI-powered rental housing search platform built with MERN stack",
  "main": "server.js",
  "keywords": [
    "rental",
    "housing",
    "ai",
    "mern",
    "react",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@mui/icons-material": "^7.2.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "faker": "^5.5.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.4",
    "notistack": "^3.0.2",
    "openai": "^4.0.0",
    "socket.io": "^4.7.2",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd client && npm install && npm run build",
    "heroku-postbuild": "npm run build",
    "render-build": "cd client && npm install && npm run build",
    "install-client": "cd client && npm install",
    "seed": "node scripts/seedDatabase.js",
    "seed-quick": "node scripts/seedDatabase.js --quick",
    "seed-clear": "node scripts/seedDatabase.js --clear-all"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  }
}
